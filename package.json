{
  "name": "notes-lib",
  "version": "0.0.1",
  "description": "Library with guitar stuff: scales, chords, progressions, etc",
  "main": "dist/index.js",
  "author": "mrfrac <fracturer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "typescript": "^3.0.1"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "testing": "jest --watchAll"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2",
    "ts-jest": "^23.0.1",
    "ts-node": "^7.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
