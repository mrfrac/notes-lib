{
  "name": "guitar-lib",
  "version": "0.0.1",
  "description": "Library with guitar stuff: scales, chords, progressions, etc",
  "main": "dist/index.js",
  "author": "mrfrac <fracturer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "typescript": "^2.6.2"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "testing": "jest --watchAll"
  },
  "devDependencies": {
    "jest": "^22.0.6",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "@types/jest": "^22.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
