{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-first",
        "named-imports-order": "lowercase-first"
      }],
      "quotemark": [true, "single"],
      "indent": [true, "spaces", 2]
  },
  "rulesDirectory": []
}